<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:PassMeta.DesktopApp.Ui.App"
             x:Class="PassMeta.DesktopApp.Ui.App.App"
             RequestedThemeVariant="Default">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme />
        <StyleInclude Source="/App/Styles/Common.xaml"/>
        <StyleInclude Source="/App/Styles/ResourceDependingStyles.xaml"/>
        <StyleInclude Source="avares://Avalonia.Controls.DataGrid/Themes/Fluent.xaml"/>
    </Application.Styles>
    
    <Application.Resources>
        <FontFamily x:Key="SegoeMdl2">/Assets/AvaRes/Fonts/WinSymbols2.ttf#Symbols</FontFamily>
    </Application.Resources>
</Application>
